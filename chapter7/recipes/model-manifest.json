{
  "RecipeFormatVersion": "2020-01-25",
  "ComponentName": "variant.DLR.ImageClassification.ModelStore",
  "ComponentVersion": "2.1.6",
  "ComponentType": "aws.greengrass.generic",
  "ComponentDescription": "Downloads the resnet50_v1 image classification ML models to the device as artifacts.",
  "ComponentPublisher": "AWS",
  "ComponentDependencies": {
    "aws.greengrass.Nucleus": {
      "VersionRequirement": ">=2.0.0 <2.6.0",
      "DependencyType": "SOFT"
    }
  },
  "Manifests": [
    {
      "Platform": {
        "os": "linux",
        "architecture": "aarch64"
      },
      "Lifecycle": {},
      "Artifacts": [
        {
          "Uri": "s3://sagemaker-<region>-<account-id>/ic-fulltraining/DLR-resnet50-aarch64-cpu-ImageClassification.zip",
          "Algorithm": "SHA-256",
          "Unarchive": "ZIP",
          "Permission": {
            "Read": "OWNER",
            "Execute": "NONE"
          }
        }
      ]
    }
  ],
  "Lifecycle": {}
}